     "data": {
      "text/latex": [
       "$\\displaystyle - 0.027843137254902 x^{3} - 0.329465764008565 x + 1.16074707322184$"
      ],
      "text/plain": [
       "-0.027843137254902*x**3 - 0.329465764008565*x + 1.16074707322184"
      ]
