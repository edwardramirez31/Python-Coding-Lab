   ],
   "source": [
    "# Sumatoria de fuerzas en x [kN]\n",
    "\n",
    "Equi_x = Ax - (D_viga[\"P\"] * sp.cos(angulo_cable))\n",
    "Ecua_x = sp.Eq(Equi_x, 0)\n",
    "Ax =  sp.solve(Ecua_x)[0]\n",
    "Ax "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "b008af89",
   "metadata": {},
   "outputs": [
